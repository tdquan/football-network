{
	"name": "football-network",
	"version": "1.0.0",
	"description": "A network for football fans",
	"engines": {
		"node": "10.14.2"
	},
	"main": "server.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"client": "npm start --prefix client",
		"server": "nodemon ./server.js",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NODE_ENV=development npm install --prefix client && npm run build --prefix client"
	},
	"author": "tdquan",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"babel-loader": "^8.0.4",
		"babel-preset-react": "^6.24.1"
	},
	"dependencies": {
		"babel-preset-stage-1": "^6.24.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"concurrently": "^4.0.1",
		"express": "^4.16.3",
		"gravatar": "^1.6.0",
		"jsonwebtoken": "^8.3.0",
		"lodash": "^4.17.11",
		"mongoose": "^5.3.1",
		"nodemon": "^1.18.4",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"validator": "^10.8.0"
	}
}
